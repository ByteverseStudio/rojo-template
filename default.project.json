{
  "name": "rojo-template",
  "tree": {
    "$className": "DataModel",
    "Lighting": {
      "$className": "Lighting",
      "$properties": {
        "Technology": "ShadowMap",
        "ShadowSoftness": 0.5,
        "GlobalShadows": true,
        "TimeOfDay": "14:00:00",
        "GeographicLatitude": 45.0,
        "Brightness": 2.0
      }
    },
    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "Packages": {
        "$path": "src/packages"
      },

      "WallyPackages": {
        "$className": "WallyPackages",
        "$path": "Packages"
      }
    },
    "ServerScriptService": {
      "$className": "ServerScriptService",
      "Server": {
        "$path": "src/server"
      },
      "WallyServerPackages": {
        "$className": "WallyServerPackages",
        "$path": "ServerPackages"
      }
    },
    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "Client": {
          "$path": "src/client"
        }
      }
    },
    "Workspace": {
      "$className": "Workspace",
      "$properties": {
        "FilteringEnabled": true
      },
      "$path": "workspace"
    },
    "TestService": {
      "$className": "TestService",
      "$path": "src/test",

      "WallyDevPackages": {
        "$className": "WallyDevPackages",
        "$path": "DevPackages"
      }
    }
  }
}